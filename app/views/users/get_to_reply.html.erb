    <% end %>
    
    <% @users_agency.each do |agent|%>
    <tr><td><%= radio_button_tag(:get_to_reply, agent.user_id) %></td>
    	<td><%= agent.member%></td><td><%= agent.branch%></td>
    	<td><%= agent.user_id%></td><td><%= agent.first_name%></td>
    	<td><%= agent.last_name%></td></tr>    
    <% end %>
</table>