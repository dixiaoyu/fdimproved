<table>
	<tr><td></td><td>Company</td><td>Branch</td><td>user Id</td><td>First Name</td><td>Last Name</td></tr>
    <% @users_natas.each do |user|%>
    <tr><td><%= radio_button_tag(:get_to_reply, user.user_id) %></td>
    	<td><%= user.member%></td><td><%= user.branch%></td>
    	<td><%= user.user_id%></td><td><%= user.first_name%></td>
